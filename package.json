{
  "name": "sampleweb3",
  "description": "Sample code using web3,nodejs and express (TypeScript)",
  "version": "0.0.0",
  "private": true,
  "main": "build/www.js",
  "scripts": {
    "dev": "ts-node src/www.ts",
    "build": "npm run lint && npm run compile",
    "compile": "tsc",
    "watch": "tsc --watch",
    "start": "node build/www.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "lint": "tslint --format stylish --exclude src/command.ts src/**/*.ts",
    "postinstall": "rm -f node_modules/web3/index.d.ts && npm run build"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.9.0",
    "pug": "~2.0.3",
    "serve-favicon": "~2.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.1",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "^0.0.29",
    "@types/gulp": "^4.0.2",
    "@types/gulp-less": "^0.0.30",
    "@types/gulp-mocha": "^0.0.30",
    "@types/gulp-nodemon": "^0.0.30",
    "@types/marked": "^0.0.28",
    "@types/mocha": "^2.2.40",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.10",
    "@types/power-assert": "^1.4.29",
    "@types/serve-favicon": "^2.2.28",
    "@types/web3": "^1.0.0",
    "browser-sync": "^2.18.8",
    "chai": "^4.1.2",
    "marked": "^0.3.6",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.1.0",
    "ts-node": "^6.1.2"
  }
}
